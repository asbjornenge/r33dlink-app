{
  "name": "ReadLink",
  "version": "1.0.0",
  "description": "The ReadLink App",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "raistlin start --hot --stylus --images",
    "build": "raistlin build --stylus --images",
    "bundle": "raistlin bundle && cp -R build/graphics dist/graphics",
    "deploy": "scp -r dist/* home:/home/asbjorn/static/r33d",
    "test": "BABEL_ENV=test mocha --compilers js:babel-core/register,styl:mocha-stylus-compiler -R nyan -w --check-leaks"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@asbjornenge/react-style": "^1.0.1",
    "@asbjornenge/react-svg": "^1.0.0",
    "artyom.js": "^0.5.1",
    "concurrently": "^0.1.1",
    "easy-read": "^0.1.7",
    "fastclick": "^1.0.6",
    "html-to-text": "^1.6.0",
    "locale-string": "^1.0.0",
    "nanoxhr": "^1.0.2",
    "query-string": "^3.0.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "babel-core": "^6.1.21",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-preset-es2015": "^6.1.18",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "browserify-hmr": "^0.3.1",
    "concurrently": "^0.1.0",
    "cssnano": "^3.3.1",
    "imgurify": "^1.2.0",
    "livereloadify": "^2.0.0",
    "mocha": "^2.3.4",
    "mocha-imguri-compiler": "^1.1.1",
    "mocha-stylus-compiler": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "resolvify": "^1.0.2",
    "stylus": "^0.51.1",
    "stylusify": "^2.1.0",
    "testdom": "^2.0.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.4.0",
    "@asbjornenge/raistlin": "^1.1.1"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "es2015"
        ]
      },
      "development": {
        "stage": 0,
        "plugins": [
          "react-transform"
        ],
        "extra": {
          "react-transform": {
            "transforms": [
              {
                "transform": "react-transform-hmr",
                "imports": [
                  "react"
                ],
                "locals": [
                  "module"
                ]
              }
            ]
          }
        }
      }
    }
  }
}
